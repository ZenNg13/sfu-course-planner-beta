{
  "name": "sfu-course-tracker",
  "version": "1.0.0",
  "description": "SFU Course Tracker with React Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "server:build": "tsc && npx tsc ../prisma/seed.ts --outDir dist/prisma --target ES2020 --module commonjs --esModuleInterop --resolveJsonModule --skipLibCheck",
    "server:start": "node dist/server.js",
    "seed": "ts-node ../prisma/seed.ts",
    "seed:prod": "node dist/prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-big-calendar": "^1.13.0",
    "zustand": "^4.5.0",
    "lucide-react": "^0.344.0",
    "date-fns": "^3.0.0",
    "@prisma/client": "^6.19.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-big-calendar": "^1.8.9",
    "@vitejs/plugin-react": "^4.3.0",
    "vite": "^5.4.0",
    "typescript": "^5.9.3",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.35",
    "autoprefixer": "^10.4.17",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}